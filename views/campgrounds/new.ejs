<%- include("../partials/header") %>
<h1 style="margin-top: 70px;padding-bottom: 20px;" class="text-center">Add a new Campground</h1>
<form action="/campgrounds" method="POST">
<div class="row">
	<div style="width:30%; margin: 0 auto;">
		<div class="form-group">
	<input  class="form-control" type="text" name="campname" placeholder="name">
	</div>
	<div class="form-group">
	<input class="form-control" type="text" name="campimage" placeholder="image">
	</div>
	<div class="form-group">
	<input class="form-control" type="text" name="campdesc" placeholder="description">
	</div>
	<div class="form-group">
	<button class="btn btn-primary btn-block">Submit</button>
	</div>
	<a href="/campgrounds">Go back</a>
    </div>
</div>
</form>
<%- include("../partials/footer") %>